{
    "sourceRoots": ["./src"],
    "angularCli": {
        "appName": "playground"
    },
    "imageSnapshotConfig": {
        "customDiffConfig": {
            "threshold": 0.1
        },
        "diffDirection": "vertical",
        "noColors": true
    },
    "visualRegressionIgnore": [
        {
            "regex": "app-feature1",
            "flags": "g"
        },
        {
            "regex": "app.+?shared.+?emoji"
        }
    ]
}
